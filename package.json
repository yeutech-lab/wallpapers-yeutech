{
  "name": "@yeutech/wallpapers-yeutech",
  "version": "0.0.0-development",
  "description": "Yeutech official wallpapers",
  "homepage": "https://yeutech.yeutech.com/wallpapers-yeutech",
  "author": "Dimitri Kopriwa <dimitri.kopriwa@yeutech.vn> (https://github.com/kopax)",
  "scripts": {
    "build": "echo No build",
    "build:readme": "toctoc README.md -w",
    "test": "echo No test",
    "prepublish": "npm run build",
    "lint-staged": "lint-staged",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build"
  },
  "repository": {
    "type": "git",
    "url": "https://module.kopaxgroup.com/yeutech/wallpapers-yeutech.git"
  },
  "bugs": {
    "url": "https://module.kopaxgroup.com/yeutech/wallpapers-yeutech/issues"
  },
  "keywords": [
    "static",
    "yeutech.vn",
    "wallpapers",
    "yeutech"
  ],
  "license": "MIT",
  "pre-commit": [
    "build:readme"
  ],
  "devDependencies": {
    "@semantic-release/changelog": "^3.0.0",
    "@semantic-release/exec": "^2.3.0",
    "@semantic-release/git": "^7.0.1",
    "@semantic-release/gitlab": "^2.1.4",
    "@semantic-release/npm": "^4.0.2",
    "@svgr/webpack": "^2.1.1",
    "@yeutech-lab/rollup-umd-documentation": "^2.3.19",
    "@yeutech/rollup-umd-ci-build": "^1.0.5",
    "@yeutech/rollup-umd-ci-deploy": "^1.0.7",
    "@yeutech/rollup-umd-ci-release": "^1.0.11",
    "@yeutech/rollup-umd-ci-release-transitive": "^1.1.6",
    "@yeutech/rollup-umd-ci-test": "^1.0.6",
    "@yeutech/rollup-umd-scripts": "^0.16.8",
    "cz-conventional-changelog": "^2.1.0",
    "pre-commit": "^1.2.2",
    "react-styleguidist": "^7.1.1",
    "semantic-release": "^15.9.1",
    "toctoc": "^0.3.0",
    "webpack": "^4.16.1"
  },
  "dependencies": {},
  "peerDependencies": {},
  "publishConfig": {
    "registry": "https://npm.kopaxgroup.com",
    "tag": "latest"
  },
  "release": {
    "branch": "master",
    "success": false,
    "fail": false,
    "npmPublish": true,
    "verifyConditions": [
      {
        "path": "@semantic-release/exec",
        "cmd": "npx @yeutech/rollup-umd-ci-release-transitive verify"
      },
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      {
        "path": "@semantic-release/gitlab",
        "gitlabUrl": "https://module.kopaxgroup.com"
      }
    ],
    "prepare": [
      {
        "path": "@semantic-release/exec",
        "cmd": "npx @yeutech/rollup-umd-ci-release-transitive prepare"
      },
      "@semantic-release/changelog",
      "@semantic-release/npm",
      {
        "path": "@semantic-release/git",
        "assets": [
          "package.json",
          "static/**/*",
          "CHANGELOG.md",
          "README.md",
          "LICENSE.md"
        ]
      }
    ],
    "publish": [
      "@semantic-release/npm",
      {
        "path": "@semantic-release/gitlab",
        "gitlabUrl": "https://module.kopaxgroup.com"
      },
      {
        "path": "@semantic-release/exec",
        "cmd": "npx @yeutech/rollup-umd-ci-release-transitive publish ${nextRelease.version}"
      }
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "declinationId": "static",
  "contributors": []
}